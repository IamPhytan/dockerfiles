# Common aliases
alias :q=exit

# TMUX
alias mux="tmuxinator"
alias tuxa="tmux a -t"

# Git
alias g="git"
alias s="git status"
alias a="git add "
alias aa="git add -A"
alias au="git add -u"
alias c="git commit "
alias cs="git commit -s "
alias cm="git commit -m "
alias csm="git commit -s -m "
alias cam="git commit -am "
alias d="git diff "
alias dword="d --word-diff"
alias dim="git diff-image "
alias p="git push"
alias pf="p --force-with-lease"
alias pl="git pull"

# APT
alias upd="sudo apt update"
alias upg="sudo apt upgrade"
alias uu="upd && upg"

